{
  "mcpServers": {
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "command": "npx"
    },
    "nx-mcp": {
      "args": [
        "nx-mcp@latest",
        "/REPLACE/WITH/YOUR/ACTUAL/NX/WORKSPACE/PATH"
      ],
      "command": "npx",
      "description": "Nx MCP Server for monorepo insights",
      "disabled": false,
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    },
    "neovim": {
      "disabled": false
    }
  }
}
